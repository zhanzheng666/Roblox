repeat task.wait() until game:IsLoaded()

if not getgenv().AlreadyExecutedScript then
    getgenv().AlreadyExecutedScript = true

    local queue_on_teleport, a = syn and syn.queue_on_teleport or queue_on_teleport or queueonteleport

    local teleportScript = game:HttpGet("https://example.com/AutoExecuteScript.html")

    queue_on_teleport(teleportScript)
end

loadstring(game:HttpGet("https://example.com/MainScript.html"))()
